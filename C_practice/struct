#define _CRT_SUCURE_NO_WARNINGS
#include <stdio.h>
#include <string.h>

struct Person
{
	char name[20];
	int age;
	char address[100];
}; 
int main()
{
	struct Person p1;
	strcpy(p1.name, "Joo");
	p1.age = 24;
	strcpy(p1.address, "경산시");

	printf("이름 -> %s\n", p1.name);
	printf("나이 -> %d\n", p1.age);
	printf("주소 -> %s\n", p1.address);
}
